            <Button x:Name="SupplyParcelButton" Content="Supply parcel by the drone" HorizontalAlignment="Right" VerticalAlignment="Center" Grid.Row="2" Width="200" Height="53" Click="SupplyParcelButton_Click" Background="#0096c7" FontFamily="Agency FB" FontWeight="Bold" FontStyle="Italic" FontSize="20"/>
