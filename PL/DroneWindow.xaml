                <TextBox x:Name="GetModel" Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center" Height="30" Width="200" Text="" TextWrapping="Wrap" Foreground="#FF3E4C2E" FontFamily="Agency FB" FontSize="22" Background="#FFF7F9F4" MouseLeave="GetModel_MouseLeave"/>
