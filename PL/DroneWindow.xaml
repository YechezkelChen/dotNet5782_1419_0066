            <Button x:Name="RealeseFromChargeButton" Content="Realese drone from charge" HorizontalAlignment="Left" VerticalAlignment="Top" Grid.Row="1" Width="200" Height="53" Click="RealeseFromChargeButton_Click" Background="#0096c7" FontFamily="Agency FB" FontWeight="Bold" FontStyle="Italic" FontSize="20"/>
